alias sc='vim ~/.bashrc_arch13'
alias bt='bluetoothctl'
alias dl='cd ~/Downloads'
alias db='cd ~/Dropbox'
alias pj='cd ~/projects'

alias magic='/usr/bin/bash ~/.magic_trackpad > /dev/null'
